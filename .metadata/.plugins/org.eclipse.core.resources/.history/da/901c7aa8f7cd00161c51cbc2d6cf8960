#PDF dosyas覺n覺n toplam sayfa say覺s覺
import PyPDF2
pdfFileObj = open('meetingminutes.pdf', 'rb')
pdfReader = PyPDF2.PdfFileReader(pdfFileObj)
print(pdfReader.numPages)

pageObj = pdfReader.getPage(1)
print(pageObj.extractText())
